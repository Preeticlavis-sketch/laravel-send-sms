{
    "name": "preeti/laravel-sms-sender",
    "description": "Send DLT SMS using Preeti\\SmsSender package via MSG91 or Twilio",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "twilio/sdk": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Preeti\\SmsSender\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Preeti\\SmsSender\\SmsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Preeti Upadhyay",
            "email": "upreeti02@gmail.com"
        }
    ]
}
